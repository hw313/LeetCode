class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int n=nums.size();
        vector<int>majorityElement;
        if(n==0) return majorityElement;
        
        sort(nums.begin(),nums.end());
        int i=0;
        int j=1;
        while(j<n){
            if(nums[j]!=nums[i]){
                if(j-i>n/3) majorityElement.push_back(nums[i]);
                i=j;
                j=i+1;
        }
            else{
                j++;
        }
        }
        if(j-i>n/3) majorityElement.push_back(nums[i]);
        return majorityElement;
    }
};
